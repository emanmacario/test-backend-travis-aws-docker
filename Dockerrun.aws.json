{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "prayingemantis/react-frontend:latest",
            "links": [
                "backend"
            ],
            "essential": false,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 8081
                }
            ],
            "memory": 256
        },
        {
            "name": "backend",
            "image": "prayingemantis/flask-backend:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000
                }
            ],
            "memory": 512
        }
    ]
}